<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">


<mapper namespace="com.reborn.web.dao.member.MemberDao">

	<insert id="insert">
		insert into Member
				(loginId,name,gender,pw,nickname,birthDay,phone,email,authorityId)
				values(#{loginId},#{name},#{gender},#{pw},#{nickname},#{birthDay},#{phone},#{email},2);
	</insert>
	
	<select id="get" resultType="com.reborn.web.entity.member.Member">
	    select * from Member
		where  loginId =#{loginId}
	</select>

</mapper>